<h1>USERS</h1>

<% if @users %>
  <table border='1' style='width:100%'>
    <th>ID</th>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Profile</th>
  <% @users.each do |user| %>
    <tr>
      <td><%=user.id%></td> 
      <td><%=user.email%></td>
      <td><%=user.firstname%></td>
      <td><%=user.lastname%></td>
      <td><%=link_to "Click", admin_user_path(user.id)%></td>
    <tr>
  <%end%> 
<%end%>
